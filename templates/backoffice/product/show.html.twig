{% extends 'base-admin.html.twig' %}

{% block title %}Product{% endblock %}

{% block body %}
    <h1>Product</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idproduct</th>
                <td>{{ product.idproduct }}</td>
            </tr>
            <tr>
                <th>Refproduct</th>
                <td>{{ product.refproduct }}</td>
            </tr>
            <tr>
                <th>Suppliername</th>
                <td>{{ product.suppliername }}</td>
            </tr>
            <tr>
                <th>Unitpriceproduct</th>
                <td>{{ product.unitpriceproduct }}</td>
            </tr>
            <tr>
                <th>Quantityproduct</th>
                <td>{{ product.quantityproduct }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_product_index') }}">back to list</a>

    <a href="{{ path('app_product_edit', {'idproduct': product.idproduct}) }}">edit</a>

    {{ include('backoffice/product/_delete_form.html.twig') }}
{% endblock %}
