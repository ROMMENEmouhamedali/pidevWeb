
{% block title %} {% endblock %}
<img src="data:image/png;base64," />

{% block body %}
    <h1>  <em> <mark>List Of reservations </mark></em>  </h1>



    {% for message in app.flashes('info')  %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}


    <table class="table" border="2"  >
        <thead>
        <tr>
            <th> <font color = "FF00CC">Reservationid</font></th>

            <th> <font color = "FF00CC">User ID</th>
            <th> <font color = "FF00CC">Room Number</th>
            <th> <font color = "FF00CC">Arrivaldate</th>
            <th> <font color = "FF00CC">Departuredate</th>
            <th> <font color = "FF00CC"> Specializedrequest</th>

        </tr>
        </thead>
        <tbody>

        {% for reservation in reservations %}
            <tr>
                <td>{{ reservation.reservationid }}</td>
                <td>{{ reservation.fkUserReservation.username }}</td>
                <td>{{ reservation.fkRoomReservation.roomnumber }}</td>
                <td>{{ reservation.arrivaldate ? reservation.arrivaldate|date('Y-m-d') : '' }}</td>
                <td>{{ reservation.departuredate ? reservation.departuredate|date('Y-m-d') : '' }}</td>
                <td>{{ reservation.specializedrequest }}</td>



                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>

        <tbody id="search">
        </tbody>
    </table>




{% endblock %}
